{
  "name": "Polus CZI Extraction Plugin",
  "version": "1.1.1",
  "title": "Extract TIFFs From CZI",
  "description": "Extracts individual fields of view from a CZI file. Saves as OME TIFF.",
  "author": "Nick Schaub (nick.schaub@nih.gov)",
  "containerId": "polusai/czi-extract-plugin:1.1.1",
  "inputs": [
    {
      "name": "inpDir",
      "type": "genericData",
      "description": "Input collection",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "outDir",
      "type": "collection",
      "description": "Output data for the plugin"
    }
  ],
  "ui": [
    {
      "key": "inputs.inpDir",
      "title": "Input collection: ",
      "description": "Collection name..."
    }
  ]
}