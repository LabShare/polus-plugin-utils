{
    "name": "Outlier removal",
    "version": "0.1.0",
    "title": "Outlier removal",
    "description": "Remove outliers from the data.",
    "author": "Jayapriya Nagarajan (jayapriya.nagarajan@nih.gov)",
    "institution": "National Center for Advancing Translational Sciences, National Institutes of Health",
    "repository": "https://github.com/labshare/polus-plugins",
    "website": "https://ncats.nih.gov/preclinical/core/informatics",
    "citation": "",
    "containerId": "labshare/polus-outlier-removal-plugin:0.1.0",
    "inputs": [      
      {
        "name": "inpdir",
        "type": "csvCollection",
        "description": "Input csv file collection",
        "required": "true"
      },
      {
        "name": "methods",
        "type": "enum",
	"description": "Select methods for outlier removal",	
        "options": {
	  "values": [
	      		"IsolationForest"
             ]
          },
	"required": "true"
      }
    ],
    "outputs": [
      {
        "name": "outdir",
        "type": "csvCollection",
        "description": "Output collection"
      }
    ],
    "ui": [
      {
        "key": "inputs.inpdir",
        "title": "Input csv file",
        "description": "Input csv file for clustering"
      },
      {
        "key": "inputs.methods",
        "title": "Remove outliers based on the methods selected",
        "description": "Select methods"
      },
      {
        "key": "inputs.outdir",
        "title": "Output csv file",
        "description": "Save output csv file containing labeled data"
      }      
    ]
  }