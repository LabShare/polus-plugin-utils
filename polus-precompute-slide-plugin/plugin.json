{
  "name": "Polus Precompute Slide Plugin",
  "version": "0.0.1",
  "title": "Precompute Slide Viewer",
  "description": "Precomputes a single image plane for viewing in Polus Volume.",
  "containerId": "labshare/polus-precompute-slide-plugin:0.0.1",
  "inputs": [

    {
      "name": "inpDir",
      "type": "collection",
      "description": "Input collection",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "outDir",
      "type": "pyramid",
      "description": "Precomputed output"
    }
  ],
  "ui": [
    {
      "key": "inputs.inpDir",
      "title": "Input collection: ",
      "description": "Collection name..."
    }
  ]
}
