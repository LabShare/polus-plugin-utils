{
    "name": "Fcs to Csv file converter",
    "version": "0.1.6",
    "title": "Fcs to Csv file converter",
    "description": "Converts fcs file to csv file",
    "author": "Jayapriya Nagarajan (jayapriya.nagarajan@nih.gov)",
    "institution": "National Center for Advancing Translational Sciences, National Institutes of Health",
    "repository": "https://github.com/labshare/polus-plugins",
    "website": "https://ncats.nih.gov/preclinical/core/informatics",
    "citation": "",
    "containerId": "labshare/polus-fcs-to-csv-converter-plugin:0.1.6",
    "inputs": [      
      {
        "name": "inpDir",
        "type": "collection",
        "description": "Fcs file collection",
        "required": "true"
      },
      {
      "name": "metaDir",
      "type": "boolean",
      "description": "Get fcs files in metadata",
      "required": "false"
      }
      ],
    "outputs": [
      {
        "name": "outDir",
        "type": "csvCollection",
        "description": "Output collection"
      }
    ],
    "ui": [
      {
        "key": "inputs.inpDir",
        "title": "Fcs file collection",
        "description": "Fcs file collection"
      },
      {
      "key": "inputs.metaDir",
      "title": "Get metadata",
      "description": "Get fcs files in metadata",
      "default": false
      }     
    ]
  }
